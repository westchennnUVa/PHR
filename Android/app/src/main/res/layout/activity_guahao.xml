<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.user.myapplication.guahao"
    android:background="@drawable/lvye">



    <Spinner
        android:layout_width="135dp"
        android:layout_height="30dp"
        android:id="@+id/spinnerhos"
        android:layout_marginTop="15dp"

        android:layout_marginLeft="40dp"
        android:background="@drawable/yuanjiaofeitouming"
        android:entries="@array/hospitals"

    />

    <ImageView
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:id="@+id/imageViewhos"
        android:background="@drawable/hos"
        android:layout_marginLeft="0dp"
        android:layout_alignTop="@+id/spinnerhos"
        />
    <Spinner
        android:layout_width="135dp"
        android:layout_height="30dp"
        android:id="@+id/spinnerkeshi"
       android:layout_alignLeft="@+id/spinnerhos"

        android:background="@drawable/yuanjiaofeitouming"
        android:entries="@array/keshi"
        android:layout_below="@+id/spinnerhos"

        android:layout_marginTop="40dp" />

    <ImageView
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:id="@+id/imageViewkeshi"
        android:background="@drawable/keshi"
        android:layout_marginLeft="0dp"
        android:layout_alignBottom="@+id/spinnerkeshi"
/>
    <Spinner
        android:layout_width="135dp"
        android:layout_height="30dp"
        android:id="@+id/spinnershijian"
        android:layout_below="@+id/spinnerkeshi"
        android:layout_marginTop="40dp"
        android:layout_alignLeft="@+id/spinnerkeshi"
        android:background="@drawable/yuanjiaofeitouming"

        android:entries="@array/shijian"/>

    <ImageView
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:id="@+id/imageViewshijian"
        android:background="@drawable/shijian"
        android:layout_marginLeft="0dp"
        android:layout_alignBottom="@+id/spinnershijian"
        android:layout_marginTop="40dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="姓名"

        android:id="@+id/textViewxingming"
        android:layout_alignLeft="@+id/spinnershijian"
        android:textColor="#ffffffff"
        android:textSize="15sp"
        android:layout_below="@+id/spinnershijian"
        android:layout_marginTop="20dp"
        />

    <EditText
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:inputType="textPersonName"
        android:hint="   Name"
        android:ems="10"
        android:layout_alignLeft="@+id/textViewxingming"
        android:id="@+id/editTextxingming"
       android:layout_below="@+id/textViewxingming"

        android:layout_marginTop="5dp"
        android:background="@drawable/yuanjiaofeitouming"
        />
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/imageViewxingming"
        android:background="@drawable/xingming"
        android:layout_alignBottom="@+id/editTextxingming"
        android:layout_marginLeft="3dp"

        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="身份证号"
        android:id="@+id/textViewshenfenzheng"
       android:layout_alignLeft="@+id/textViewxingming"
        android:layout_below="@+id/editTextxingming"
        android:layout_marginTop="20dp"
        android:textColor="#ffffffff"
        android:textSize="15sp"

        />

    <EditText
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:inputType="textPersonName"
        android:hint="   ID"
        android:ems="10"
        android:layout_alignLeft="@+id/textViewshenfenzheng"
        android:id="@+id/editTextshenfenzheng"
        android:layout_below="@+id/textViewshenfenzheng"
        android:layout_marginTop="5dp"
        android:background="@drawable/yuanjiaofeitouming"
        />
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/imageViewshenfenzheng"
        android:background="@drawable/shenfenzheng"
        android:layout_alignBottom="@id/editTextshenfenzheng"
        android:layout_marginLeft="3dp"


        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="电话号码"
        android:id="@+id/textViewdianhua"
       android:layout_alignLeft="@+id/textViewshenfenzheng"
        android:layout_below="@+id/editTextshenfenzheng"
        android:layout_marginTop="20dp"
        android:textColor="#ffffffff"
        android:textSize="15sp"

        />

    <EditText
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:inputType="textPersonName"
        android:hint="   Phone"
        android:ems="10"
        android:layout_alignLeft="@+id/textViewdianhua"
        android:id="@+id/editTextdianhua"
        android:layout_below="@+id/textViewdianhua"
        android:layout_marginTop="10dp"
        android:background="@drawable/yuanjiaofeitouming"
        />
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/imageViewdianhua"
        android:background="@drawable/dianhua"
        android:layout_alignBottom="@+id/editTextdianhua"
        android:layout_marginLeft="3dp"

        />
    <Button
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:text="提交订单"
        android:textColor="#ffff07"
        android:textSize="20sp"
        android:id="@+id/button"
       android:layout_toRightOf="@+id/spinnerkeshi"
       android:layout_marginLeft="20dp"
        android:layout_alignTop="@+id/spinnerhos"
        android:layout_alignBottom="@+id/spinnershijian"
        android:background="@drawable/yuanjiao"/>

</RelativeLayout>
